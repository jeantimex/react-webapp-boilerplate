{
  "plugins": [
    "transform-class-properties",
    ["transform-runtime", {
      "polyfill": false
    }],
    ["react-intl", {
      "messagesDir": "./.tmp/messages/"
    }]
  ],
  "presets": [
    ["latest", {
      "es2015": {
        "modules": false
      }
    }],
    "react",
    "stage-0"
  ],
  "env": {
    "test": {
      "presets": [
        "latest",
        "react",
        "stage-0"
      ]
    }
  }
}
